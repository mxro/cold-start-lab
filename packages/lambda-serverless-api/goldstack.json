{
  "$schema": "./schemas/package.schema.json",
  "name": "serverless-api",
  "template": "serverless-api",
  "templateVersion": "0.1.18",
  "configuration": {},
  "deployments": [
    {
      "name": "prod",
      "configuration": {
        "lambdas": {
          "ANY /dynamodb": {
            "function_name": "cold-start-lab-dynamodb",
            "route": "ANY /dynamodb"
          },
          "ANY /hello-world": {
            "function_name": "cold-start-lab-hello-world",
            "route": "ANY /hello-world"
          },
          "ANY /s3": {
            "function_name": "cold-start-lab-s3",
            "route": "ANY /s3"
          }
        },
        "apiDomain": "serverless-api.cold-start-lab.mxro.de",
        "lambdaNamePrefix": "cold-start-lab",
        "hostedZoneDomain": "mxro.de",
        "cors": "https://cold-start-lab.mxro.de"
      },
      "awsUser": "awsUser",
      "awsRegion": "us-west-2",
      "tfStateKey": "serverless-api-prod-9a601c1a31ecb9b4c73e.tfstate"
    }
  ]
}